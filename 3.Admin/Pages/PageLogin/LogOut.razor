@code {
    [Inject] public NavigationManager NavigationManager { get; set; }
    [Inject] public IAllServices services { get; set; }
    [Inject] public IJSRuntime JSRuntime { get; set; }
    protected Confirmation confirmation { get; set; }


    protected override async Task OnInitializedAsync()
    {
    await JSRuntime.InvokeAsync<object>("sessionStorage.removeItem", "idnv");
    await JSRuntime.InvokeAsync<object>("sessionStorage.removeItem", "tennv");
    NavigationManager.NavigateTo("/");
    }

    
}
